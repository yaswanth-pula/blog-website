<%- include('partials/header'); %>
    <h1>Home</h1>

    <p><%= homeStartingContent %> </p>
    <form action="/compose" method="GET">
        <button class=" btn btn-primary" type="submit">New Post </button>
    </form> 
    <% htmlPosts.forEach(htmlPost => { %>
        <h1><%= htmlPost.title %> </h1> 
        <p>
            <%= htmlPost.content.substring(0,100)+" ..." %> 
            <a href="/posts/<%=htmlPost._id%>">Read More</a> </p>
    <% }) %>
<%- include('partials/footer'); %>